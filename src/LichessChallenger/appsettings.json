  "NLog": {
    "autoreload": true,
    "internalLogLevel": "Error",
    "internalLogFile": "${basedir}/logs/internal-nlog.txt",
    "throwExceptions": true,
    "variables": {
      "logDirectory": "${basedir}/logs",
      "archiveLogDirectory": "${basedir}/logs/archives"
    },
    "targets": {
      "errors": {
        "type": "File",
        "layout": "${longdate}|${event-properties:item=EventId_Id:whenEmpty=0}|${uppercase:${level}}|${logger}| ${message} ${exception:format=tostring}",
        "fileName": "${logDirectory}/errors-${date:format=yyyy-MM-dd}.log",
        "concurrentWrites": true,
        "keepFileOpen": false,
        "archiveFileName": "${archiveLogDirectory}/archived-errors-{#}.log",
        "archiveEvery": "Monday",
        "archiveNumbering": "Date",
        "archiveDateFormat": "yyyy-MM-dd",
        "maxArchiveFiles": 100
      },
      "logs": {
        "type": "File",
        "layout": "${longdate}|${event-properties:item=EventId_Id:whenEmpty=0}|${uppercase:${level}}| ${message} ${exception:format=tostring}",
        "fileName": "${logDirectory}/logs-${date:format=yyyy-MM-dd}.log",
        "concurrentWrites": true,
        "keepFileOpen": false,
        "archiveFileName": "${archiveLogDirectory}/archived-logs-{#}.log",
        "archiveEvery": "Monday",
        "archiveNumbering": "Date",
        "archiveDateFormat": "yyyy-MM-dd",
        "maxArchiveFiles": 100
      },
      "console": {
        "type": "ColoredConsole",
        "layout": "${date:format=HH\\:mm\\:ss} | [${uppercase:${level}}] ${message} ${exception:format=tostring}",
        "rowHighlightingRules": [
          {
            "condition": "level == LogLevel.Fatal",
            "foregroundColor": "Red",
            "backgroundColor": "White"
          },
          {
            "condition": "level == LogLevel.Error",
            "foregroundColor": "Red"
          },
          {
            "condition": "level == LogLevel.Warn",
            "foregroundColor": "Yellow"
          },
          {
            "condition": "level == LogLevel.Info",
            "foregroundColor": "Gray"
          },
          {
            "condition": "level == LogLevel.Debug",
            "foregroundColor": "DarkGray"
          }
        ]
      }
    },
    "rules": {
      "0": {
        "logger": "*",
        "minLevel": "Warn",
        "writeTo": "errors"
      },
      "1": {
        "logger": "LichessChallenger*",
        "minLevel": "Info",
        "writeTo": "logs"
      },
      "2": {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "console"
      }
    }
  }
}
